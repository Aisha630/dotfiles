{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "\u2570\u2500‚ùØ ",
    "foreground": "#D6DEEB",
    "background": "transparent"
  },
  "transient_prompt": {
    "newline": true,
    "template": " \uf252 {{ .Segments.TimeLastCommand.FormattedMs }} \n \ue780 ",
    "foreground": "#dabd17",
    "background": "transparent"
  },

  "console_title_template": "{{ if .Root }}root @ {{ end }}{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "arch": "\uf303",
            "cache_duration": "none",
            "debian": "\uf306",
            "linux": "\ue712",
            "macos": "\ue711",
            "ubuntu": "\uf31c"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }} ",
          "foreground": "#D6DEEB",
          "background": "#7469B6",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf09c admin ",
          "foreground": "#D6DEEB",
          "powerline_symbol": "\ue0b0",
          "background": "#EF541C",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue61e \ue61d cmake {{ .Full }} ",
          "foreground": "#E8EAEE",
          "powerline_symbol": "\ue0b0",
          "background": "#1E9748",
          "type": "cmake",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": " \ue73c {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }} ",
          "foreground": "#011627",
          "powerline_symbol": "\ue0b0",
          "background": "#B787D7",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \u202d\ue626 {{ .Full }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#7FD5EA",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue7a8 {{ .Full }} ",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#99908A",
          "type": "rust",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#055b9c",
          "background": "#e1e8e9",
          "type": "dart",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ü•ü {{ .Full }} ",
          "foreground": "#e5b777",
          "type": "bun",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#06A4CE>\ue0b2</>",
          "template": "\ue28e {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "background": "#06A4CE",
          "type": "flutter",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#fe562e>\ue0b2</>",
          "template": "\ue755 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "background": "#fe562e",
          "type": "swift",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "display_status": true,
            "fetch_status": true
          },
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }} ",
          "foreground": "#011627",
          "powerline_symbol": "\ue0b0",
          "background": "#E493B3",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#8ACDD7{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#8ACDD7{{ end }}",
            "{{ if gt .Ahead 0 }}#FFEFEF{{ end }}",
            "{{ if gt .Behind 0 }}#FFEFEF{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "foreground": "#414141",
          "background": "#BEADFA",
          "type": "status",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#D6DEEB{{ end }}"
          ],
          "background_templates": [
            "{{ if gt .Code 0 }}#E44141{{ end }}"
          ]
        },
        {
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "alias": "TimeLastCommand",
          "trailing_diamond": "\ue0b4",
          "template": " \uf252 {{ .FormattedMs }} ",
          "foreground": "#D6DEEB",
          "background": "#575656",
          "type": "executiontime",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "  {{ .Project }} :: {{.Account}} ",
          "foreground": "#F4B400",
          "type": "gcp",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .Stack }}{{if .User }} :: {{ .User }}@{{ end }}{{ if .URL }}{{ .URL }}{{ end }}",
          "foreground": "#8759aa",
          "type": "pulumi",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\uf109 ",
          "template": "{{ if .SSHSession }}{{ end }}{{ .UserName }}",
          "foreground": "#d964a0",
          "type": "session",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u256d\u2500",
          "foreground": "#D6DEEB",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "<#D6DEEB>\ue641 15:04:05</> <#79DFE1>|</> \uf073 2 Jan, Monday"
          },
          "template": "{{ .CurrentDate | date .Format }} <#79DFE1>|</>",
          "foreground": "#F2D3B6",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": " \uf07c ",
            "folder_separator_icon": " \uf061 ",
            "home_icon": "\ueb06 ",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "leading_diamond": "<#00C7FC> \uf07b </><#B6D6F2> in </>",
          "template": " {{ .Path }} ",
          "foreground": "#B6D6F2",
          "type": "path",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "foreground": "#D6DEEB",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "‚ùØ ",
          "foreground": "#D6DEEB",
          "type": "status",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}